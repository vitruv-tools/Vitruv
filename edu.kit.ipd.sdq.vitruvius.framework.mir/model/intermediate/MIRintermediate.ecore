<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="MIRintermediate" nsURI="http://www.kit.edu/ipd/sdq/vitruvius/framework/mir/MIRintermediate"
    nsPrefix="MIRintermediate">
  <eClassifiers xsi:type="ecore:EClass" name="MIR">
    <eStructuralFeatures xsi:type="ecore:EReference" name="classMappings" upperBound="-1"
        eType="#//ClassMapping" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="featureMappings" upperBound="-1"
        eType="#//ReferenceMapping" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="predicates" upperBound="-1"
        eType="#//Predicate" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="configuration" lowerBound="1"
        eType="#//Configuration" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="packages" lowerBound="2"
        upperBound="2" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EPackage"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mapping" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="predicates" upperBound="-1"
        eType="#//Predicate"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="postconditions" upperBound="-1"
        eType="#//PostCondition" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ReferenceMapping" eSuperTypes="#//Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" upperBound="-1"
        eType="#//NamedEReference" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" upperBound="-1"
        eType="#//NamedEReference" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="classMapping" eType="#//ClassMapping"
        eOpposite="#//ClassMapping/featureMapping"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//ClassMapping"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ClassMapping" eSuperTypes="#//Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="#//NamedEClass"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="#//NamedEClass"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="featureMapping" eType="#//ReferenceMapping"
        eOpposite="#//ReferenceMapping/classMapping"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="opposite" lowerBound="1"
        eType="#//ClassMapping"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Predicate" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="WhenWhereJavaPredicate" eSuperTypes="#//Predicate">
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignmentExpression" lowerBound="1"
        eType="#//StaticMethodCall" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="checkExpression" lowerBound="1"
        eType="#//StaticMethodCall" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Configuration">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="package" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NamedEReference" eSuperTypes="#//NamedTyped">
    <eStructuralFeatures xsi:type="ecore:EReference" name="eClass" lowerBound="1"
        eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EClass"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="eReference" lowerBound="1"
        eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EReference"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PostCondition" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="WithBlockPostCondition" eSuperTypes="#//PostCondition">
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignmentExpression" lowerBound="1"
        eType="#//StaticMethodCall" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NamedEClass" eSuperTypes="#//NamedTyped"/>
  <eClassifiers xsi:type="ecore:EClass" name="NamedTyped" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EClass"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StaticMethodCall">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="methodFQN" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString" defaultValueLiteral=""/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="parameterNames" lowerBound="1"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WithAttributePostCondition" eSuperTypes="#//PostCondition">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EAttribute"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EAttribute"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="leftVariableName" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rightVariableName" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
