<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.sirius.common.resourceSetFactory">
		<factory class="tools.vitruvius.views.java.projumled4j.editor.diagramGeneration.jamoppExtension.SiriusResourceSetFactory" />
	</extension>
	
  <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="tools.vitruvius.views.java.projumled4j.editor.diagramGenerationCommands"
            defaultHandler="tools.vitruvius.views.java.projumled4j.editor.diagramGeneration.commands.OpenUmlDiagramAction"
            id="tools.vitruvius.views.java.projumled4j.editor.diagramGeneration.commands.OpenUmlDiagramCommand"
            name="Show UML Class Diagramm">
      </command>
      <category
            id="tools.vitruvius.views.java.projumled4j.editor.diagramGenerationCommands"
            name="Diagram Generation Commands">
      </category>
   </extension>
	<extension point="org.eclipse.ui.menus">
    	<menuContribution locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
      		<command commandId="tools.vitruvius.views.java.projumled4j.editor.diagramGeneration.commands.OpenUmlDiagramCommand"
        		label="Show UML Class diagram" style="push">
      		</command>
    	</menuContribution>
     <menuContribution
           locationURI="popup:eclipse.sirius.ui.tools.views.model.explorer">
        <command
              commandId="tools.vitruvius.views.java.projumled4j.editor.diagramGeneration.commands.OpenUmlDiagramCommand"
              label="Show UML Class diagram"
              style="push">
        </command>
     </menuContribution>
  	</extension>
</plugin>
