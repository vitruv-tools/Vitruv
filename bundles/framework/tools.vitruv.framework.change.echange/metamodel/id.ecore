        resolveProxies="false"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EObjectToIdMapEntry" instanceClassName="java.util.Map$Entry">
    <eStructuralFeatures xsi:type="ecore:EReference" name="key" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"
        resolveProxies="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="#//Id"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="Id" instanceClassName="java.lang.String"/>
