module generate

import org.eclipse.emf.mwe2.ecore.EcoreGenerator
import org.eclipse.emf.mwe.utils.StandaloneSetup

Workflow {

	bean = StandaloneSetup {
		scanClassPath = true
		platformUri = "."
	}

	component = EcoreGenerator {
		srcPath = "platform:/resource/tools.vitruv.framework.uuid/src-gen"
		genModel = "platform:/resource/tools.vitruv.framework.uuid/model/Uuid.genmodel"
		generateCustomClasses = false
	}

}
